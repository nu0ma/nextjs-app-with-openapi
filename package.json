    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "axios": "^0.22.0",
    "framer-motion": "^4",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.17.0",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.2",
    "@types/react": "17.0.26",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "lint-staged": "^11.2.0",
    "msw": "^0.35.0",
